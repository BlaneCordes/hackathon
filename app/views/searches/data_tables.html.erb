<% if @query_response %>
  <table class="table table-striped">
    <thead>
      <% @query_keys.each do |qk| %>
        <th><%= MLB_MAPPING[qk.to_sym] %></th>
      <% end %>
    </thead>
    <% @query_response.each do |qr| %>
      <tr>
        <% @query_keys.each do |k| %>
          <td><%= qr.send(k) %> </td>
        <% end %>
      </tr>
    <% end %>
  </table>
<% end %>

